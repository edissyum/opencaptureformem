{
    "FIELDS": {
        "CONTACT": {
            "table": "contacts",
            "data": {
                "lastname": {
                    "regex": "Nom\\s:\\s(.+)",
                    "column": "lastname"
                },
                "firstname": {
                    "regex": "Prénom\\s:\\s(.+)",
                    "column": "firstname"
                },
                "email": {
                    "regex": "E-mail\\s:\\s(.+)",
                    "column": "email"
                },
                "address_street": {
                    "regex": "Adresse\\s:\\s(.+)",
                    "column": "addressStreet"
                },
                "address_postcode": {
                    "regex": "Code postal\\s:\\s(.+)",
                    "column": "addressPostcode"
                },
                "address_town": {
                    "regex": "Ville\\s:\\s(.+)",
                    "column": "addressTown"
                },
                "address_country": {
                    "regex": "Pays\\s:\\s(.+)",
                    "column": "addressCountry"
                },
                "Civilité": {
                    "regex": "Civilité\\s:\\s(.+)",
                    "column": "civility",
                    "correspondance_table": {
                        "Monsieur": 1,
                        "Madame": 2,
                        "Mademoiselle": 3,
                        "Messieurs": 4,
                        "Mesdames": 5,
                        "Mesdemoiselles": 6
                    }
                }
            }
        },
        "LETTERBOX": {
            "table": "res_letterbox",
            "data": {}
        }
    }
}